{
  "name": "ImageComponent",
  "version": "2.2.0",
  "parameters": {
    "name": {
      "type": "string"
    },
    "stimulus": {
      "type": "string"
    },
    "height": {
      "type": "number",
      "default": null
    },
    "width": {
      "type": "number",
      "default": null
    },
    "maintain_aspect_ratio": {
      "type": "boolean",
      "default": true
    },
    "stimulus_duration": {
      "type": "number",
      "default": null
    },
    "coordinates": {
      "type": "object",
      "default": "{ x: 0, y: 0 }",
      "pretty_name": "Coordinates",
      "description": "Object with x and y properties for absolute positioning"
    },
    "zIndex": {
      "type": "number",
      "default": 0,
      "pretty_name": "Z-Index",
      "description": "Layer order - higher values render on top of lower values"
    }
  }
}