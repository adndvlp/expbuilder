{
  "name": "HtmlComponent",
  "version": "2.1.0",
  "parameters": {
    "name": {
      "type": "string"
    },
    "stimulus": {
      "type": "html_string"
    },
    "coordinates": {
      "type": "object",
      "default": "{ x: 0, y: 0 }"
