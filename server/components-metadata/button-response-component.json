{
  "name": "ButtonResponseComponent",
  "version": "2.2.0",
  "parameters": {
    "name": {
      "type": "string"
    },
    "choices": {
      "type": "string_array"
    },
    "button_html": {
      "type": "function",
      "default": "function (choice: string, choice_index: number) {\n        return `<button class=\"jspsych-btn\">${choice}</button>`;\n      }"
    },
    "image_button_width": {
      "type": "number",
      "default": 150
    },
    "image_button_height": {
      "type": "number",
      "default": 150
    },
    "button_layout": {
      "type": "string",
      "default": "grid"
    },
    "grid_rows": {
      "type": "number",
      "default": 1
    },
    "grid_columns": {
      "type": "number",
      "default": null
    },
    "enable_button_after": {
      "type": "number",
      "default": 0
    },
    "coordinates": {
      "type": "object",
      "default": "{ x: 0, y: 0 }"
    }
  },
  "data": {
    "response": {
      "type": "string"
    },
    "rt": {
      "type": "number"
    }
  }
}