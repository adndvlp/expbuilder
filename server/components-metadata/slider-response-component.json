{
  "name": "SliderResponseComponent",
  "version": "2.1.1",
  "parameters": {
    "name": {
      "type": "string"
    },
    "min": {
      "type": "number",
      "default": 0
    },
    "max": {
      "type": "number",
      "default": 100
    },
    "slider_start": {
      "type": "number",
      "default": 50
    },
    "step": {
      "type": "number",
      "default": 1
    },
    "labels": {
      "type": "string_array",
      "default": []
    },
    "slider_width": {
      "type": "number",
      "default": null
    },
    "require_movement": {
      "type": "boolean",
      "default": false
    },
    "coordinates": {
      "type": "object",
      "default": "{ x: 0, y: 0 }"
    },
    "zIndex": {
      "type": "number",
      "default": 0,
      "pretty_name": "Z-Index",
      "description": "Layer order - higher values render on top of lower values"
    }
  },
  "data": {
    "response": {
      "type": "number"
    },
    "rt": {
      "type": "number"
    },
    "slider_start": {
      "type": "number"
    }
  }
}