{
  "name": "InputResponseComponent",
  "version": "2.2.0",
  "parameters": {
    "name": {
      "type": "string"
    },
    "text": {
      "type": "html_string"
    },
    "check_answers": {
      "type": "boolean",
      "default": false
    },
    "allow_blanks": {
      "type": "boolean",
      "default": true
    },
    "case_sensitivity": {
      "type": "boolean",
      "default": true
    },
    "autofocus": {
      "type": "boolean",
      "default": true
    },
    "coordinates": {
      "type": "object",
      "default": "{ x: 0, y: 0 }"
    },
    "zIndex": {
      "type": "number",
      "default": 0,
      "pretty_name": "Z-Index",
      "description": "Layer order - higher values render on top of lower values"
    }
  },
  "data": {
    "response": {
      "type": "string_array"
    },
    "rt": {
      "type": "number"
    }
  }
}