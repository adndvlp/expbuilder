{
  "name": "free-sort",
  "version": "2.1.0",
  "parameters": {
    "stimuli": {
      "type": "image_array",
      "default": "__undefined__"
    },
    "stim_height": {
      "type": "number",
      "default": 100
    },
    "stim_width": {
      "type": "number",
      "default": 100
    },
    "scale_factor": {
      "type": "number",
      "default": 1.5
    },
    "sort_area_height": {
      "type": "number",
      "default": 700
    },
    "sort_area_width": {
      "type": "number",
      "default": 700
    },
    "sort_area_shape": {
      "type": "select",
      "options": [
        "square",
        "ellipse"
      ],
      "default": "ellipse"
    },
    "prompt": {
      "type": "html_string",
      "default": ""
    },
    "prompt_location": {
      "type": "select",
      "options": [
        "above",
        "below"
      ],
      "default": "above"
    },
    "button_label": {
      "type": "string",
      "default": "Continue"
    },
    "change_border_background_color": {
      "type": "boolean",
      "default": true
    },
    "border_color_in": {
      "type": "string",
      "default": "#a1d99b"
    },
    "border_color_out": {
      "type": "string",
      "default": "#fc9272"
    },
    "border_width": {
      "type": "number",
      "default": null
    },
    "counter_text_unfinished": {
      "type": "html_string",
      "default": "You still need to place %n% item%s% inside the sort area."
    },
    "counter_text_finished": {
      "type": "html_string",
      "default": "All items placed. Feel free to reposition items if necessary."
    },
    "stim_starts_inside": {
      "type": "boolean",
      "default": false
    },
    "column_spread_factor": {
      "type": "number",
      "default": 1
    }
  },
  "data": {
    "init_locations": {
      "type": "string",
      "array": true
    },
    "moves": {
      "type": "complex",
      "array": true,
      "nested": {
        "src": {
          "type": "string"
        },
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      }
    },
    "final_locations": {
      "type": "complex",
      "array": true,
      "nested": {
        "src": {
          "type": "string"
        },
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      }
    },
    "rt": {
      "type": "number"
    }
  },
  "citations": {
    "apa": "de Leeuw, J. R., Gilbert, R. A., & Luchterhandt, B. (2023). jsPsych: Enabling an Open-Source Collaborative Ecosystem of Behavioral Experiments. Journal of Open Source Software, 8(85), 5351. https://doi.org/10.21105/joss.05351 ",
    "bibtex": "@article{Leeuw2023jsPsych, \tauthor = {de Leeuw, Joshua R. and Gilbert, Rebecca A. and Luchterhandt, Bj{\\\" o}rn}, \tjournal = {Journal of Open Source Software}, \tdoi = {10.21105/joss.05351}, \tissn = {2475-9066}, \tnumber = {85}, \tyear = {2023}, \tmonth = {may 11}, \tpages = {5351}, \tpublisher = {Open Journals}, \ttitle = {jsPsych: Enabling an {Open}-{Source} {Collaborative} {Ecosystem} of {Behavioral} {Experiments}}, \turl = {https://joss.theoj.org/papers/10.21105/joss.05351}, \tvolume = {8}, }  "
  }
}